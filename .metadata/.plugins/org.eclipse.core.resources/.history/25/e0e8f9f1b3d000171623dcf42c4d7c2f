amique.</Description><Etape>9</Etape></value></etapes><ingredients><value><name> carré frais</name><quantity>3</quantity></value><value><name> tranche de saumon fumé</name><quantity>4</quantity></value><value><name> pot de poivron rouge</name><quantity>1</quantity></value><value><name> poireau</name><quantity>2</quantity></value><value><name> feuille de gélatine</name><quantity>1</quantity></value><value><name>Ciboulette</name></value><value><name>Aneth</name></value><value><name>Basilic</name></value><value><name>Poivre</name></value><value><name>Sel</name></value><value><name>Mâche</name></value></ingredients><time><total>20 min</total><preparation>20 min</preparation></time></item>
